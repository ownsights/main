{
  "name": "ownsights-main",
  "version": "0.0.1",
  "description": "OwnSights main repository",
  "repository": "git@gitlab.com:ownsights/main.git",
  "author": "Szymon Nowicki <sznowicki@eskalacja.com>",
  "license": "Apache-2",
  "private": true,
  "scripts": {
    "dev": "docker-compose up",
    "stop": "docker-compose down",
    "jest:run": "docker-compose -f docker-compose-jest.yml up",
    "jest:stop": "docker-compose -f docker-compose-jest.yml down",
    "jest:test": "docker exec ownsights-jest sh -c 'cd /app && yarn jest'",
    "jest:watch": "docker exec ownsights-jest sh -c 'cd /app && yarn jest --watch'",
    "jest:cover": "docker exec ownsights-jest sh -c 'cd /app && yarn jest --coverage'"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^25.1.0"
  },
  "dependencies": {}
}
