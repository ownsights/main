{
  "name": "ownsights-main",
  "version": "0.0.1",
  "description": "OwnSights main repository",
  "repository": "git@gitlab.com:ownsights/main.git",
  "author": "Szymon Nowicki <sznowicki@eskalacja.com>",
  "license": "Apache-2",
  "private": true,
  "scripts": {
    "start": "docker-compose up",
    "stop": "docker-compose down",
    "dev": "docker-compose -f docker-compose-actions.yml up",
    "test": "docker exec ownsights-server-dev sh -c 'cd /app && yarn test:run'",
    "watch": "docker exec ownsights-server-dev sh -c 'cd /app && yarn test:run --watch'",
    "cover": "docker exec ownsights-server-dev sh -c 'cd /app && yarn test:run --coverage'",
    "test:run": "jest --runInBand"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2"
  },
  "dependencies": {}
}
